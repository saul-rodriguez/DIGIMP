#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x2608f00 .scope module, "test" "test" 2 1;
 .timescale 0 0;
v0x261c760_0 .var "clk", 0 0;
v0x261c830_0 .var "in", 0 0;
v0x261c8e0_0 .net "out", 3 0, L_0x261ca40; 1 drivers
v0x261c960_0 .var "reset", 0 0;
L_0x261ca40 .part v0x261c610_0, 0, 4;
S_0x2608ff0 .scope module, "my_ser" "ser" 2 25, 3 1, S_0x2608f00;
 .timescale 0 0;
v0x26090e0_0 .net "clk", 0 0, v0x261c760_0; 1 drivers
v0x261c570_0 .net "in", 0 0, v0x261c830_0; 1 drivers
v0x261c610_0 .var "out", 7 0;
v0x261c6b0_0 .net "reset", 0 0, v0x261c960_0; 1 drivers
E_0x26094e0 .event posedge, v0x26090e0_0;
    .scope S_0x2608ff0;
T_0 ;
    %wait E_0x26094e0;
    %load/v 8, v0x261c6b0_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x261c610_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x261c610_0, 8;
    %ix/load 0, 1, 0;
    %mov 4, 0, 1;
    %shiftl/i0  8, 8;
    %load/v 16, v0x261c570_0, 1;
    %mov 17, 0, 7;
    %or 8, 16, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x261c610_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x2608f00;
T_1 ;
    %vpi_call 2 8 "$dumpfile", "ser.vcd";
    %vpi_call 2 9 "$dumpvars", 1'sb0, S_0x2608ff0;
    %vpi_call 2 10 "$display", "reset in out[3-0]";
    %vpi_call 2 11 "$monitor", "%b   %b   %b   %b   %b   %b", v0x261c960_0, v0x261c830_0, &PV<v0x261c8e0_0, 3, 1>, &PV<v0x261c8e0_0, 2, 1>, &PV<v0x261c8e0_0, 1, 1>, &PV<v0x261c8e0_0, 0, 1>;
    %set/v v0x261c760_0, 0, 1;
    %set/v v0x261c830_0, 0, 1;
    %set/v v0x261c960_0, 0, 1;
    %delay 3, 0;
    %set/v v0x261c960_0, 1, 1;
    %delay 10, 0;
    %set/v v0x261c960_0, 0, 1;
    %delay 5, 0;
    %set/v v0x261c830_0, 1, 1;
    %delay 100, 0;
    %vpi_call 2 18 "$finish";
    %end;
    .thread T_1;
    .scope S_0x2608f00;
T_2 ;
    %delay 5, 0;
    %load/v 8, v0x261c760_0, 1;
    %inv 8, 1;
    %set/v v0x261c760_0, 8, 1;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "ser_tb.v";
    "ser.v";
