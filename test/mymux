#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x16626d0 .scope module, "counter_tb" "counter_tb" 2 8;
 .timescale 0 0;
v0x1673320_0 .var "clk", 0 0;
v0x16733f0_0 .var "enable", 0 0;
v0x16734a0_0 .net "out", 7 0, v0x16731d0_0; 1 drivers
v0x1673550_0 .var "reset", 0 0;
S_0x16627c0 .scope module, "my_counter" "counter" 2 31, 3 8, S_0x16626d0;
 .timescale 0 0;
v0x163fbc0_0 .net "clk", 0 0, v0x1673320_0; 1 drivers
v0x1673130_0 .net "enable", 0 0, v0x16733f0_0; 1 drivers
v0x16731d0_0 .var "out", 7 0;
v0x1673270_0 .net "reset", 0 0, v0x1673550_0; 1 drivers
E_0x163e710 .event posedge, v0x163fbc0_0;
    .scope S_0x16627c0;
T_0 ;
    %wait E_0x163e710;
    %load/v 8, v0x1673270_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x16731d0_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x1673130_0, 1;
    %jmp/0xz  T_0.2, 8;
    %load/v 8, v0x16731d0_0, 8;
    %mov 16, 0, 24;
    %addi 8, 1, 32;
    %ix/load 0, 8, 0;
    %assign/v0 v0x16731d0_0, 0, 8;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x16626d0;
T_1 ;
    %vpi_call 2 16 "$dumpfile", "counter.vcd";
    %vpi_call 2 17 "$dumpvars", 1'sb0, S_0x16627c0;
    %set/v v0x1673320_0, 0, 1;
    %set/v v0x1673550_0, 0, 1;
    %set/v v0x16733f0_0, 0, 1;
    %delay 16, 0;
    %set/v v0x1673550_0, 1, 1;
    %delay 18, 0;
    %set/v v0x1673550_0, 0, 1;
    %delay 10, 0;
    %set/v v0x16733f0_0, 1, 1;
    %delay 100, 0;
    %vpi_call 2 24 "$finish";
    %end;
    .thread T_1;
    .scope S_0x16626d0;
T_2 ;
    %delay 5, 0;
    %load/v 8, v0x1673320_0, 1;
    %inv 8, 1;
    %set/v v0x1673320_0, 8, 1;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "counter_tb.v";
    "counter.v";
