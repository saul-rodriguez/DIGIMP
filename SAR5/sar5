#! /usr/local/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xdc6f20 .scope module, "sar5_tb" "sar5_tb" 2 9;
 .timescale 0 0;
v0xdeb0b0_0 .var "clk", 0 0;
v0xdeb170_0 .var "comp", 0 0;
v0xdeb240_0 .net "out", 4 0, L_0xdeb6c0;  1 drivers
v0xdeb340_0 .var "resetn", 0 0;
v0xdeb410_0 .net "sar_serial", 0 0, v0xdeada0_0;  1 drivers
v0xdeb4b0_0 .net "start_ready", 0 0, L_0xdeb580;  1 drivers
S_0xdc2310 .scope module, "my_sar5" "sar5" 2 44, 3 20 0, S_0xdc6f20;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "comp"
    .port_info 2 /INPUT 1 "resetn"
    .port_info 3 /OUTPUT 5 "sar"
    .port_info 4 /OUTPUT 1 "sar_serial"
    .port_info 5 /OUTPUT 1 "start_ready"
L_0xdeb6c0 .functor OR 5, v0xdeab20_0, v0xdea9f0_0, C4<00000>, C4<00000>;
L_0x7f8b54196018 .functor BUFT 1, C4<101>, C4<0>, C4<0>, C4<0>;
v0xdc2580_0 .net/2u *"_s0", 2 0, L_0x7f8b54196018;  1 drivers
v0xdea780_0 .net "clk", 0 0, v0xdeb0b0_0;  1 drivers
v0xdea840_0 .net "comp", 0 0, v0xdeb170_0;  1 drivers
v0xdea910_0 .var "counter", 2 0;
v0xdea9f0_0 .var "dac", 4 0;
v0xdeab20_0 .var "dac_test", 4 0;
v0xdeac00_0 .net "resetn", 0 0, v0xdeb340_0;  1 drivers
v0xdeacc0_0 .net "sar", 4 0, L_0xdeb6c0;  alias, 1 drivers
v0xdeada0_0 .var "sar_serial", 0 0;
v0xdeaef0_0 .net "start_ready", 0 0, L_0xdeb580;  alias, 1 drivers
E_0xdc29d0 .event edge, v0xdea910_0;
E_0xdc2db0/0 .event negedge, v0xdeac00_0;
E_0xdc2db0/1 .event posedge, v0xdea780_0;
E_0xdc2db0 .event/or E_0xdc2db0/0, E_0xdc2db0/1;
L_0xdeb580 .cmp/eq 3, v0xdea910_0, L_0x7f8b54196018;
    .scope S_0xdc2310;
T_0 ;
    %wait E_0xdc2db0;
    %load/vec4 v0xdeac00_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 5;
    %assign/vec4 v0xdea9f0_0, 0;
    %pushi/vec4 5, 0, 3;
    %assign/vec4 v0xdea910_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0xdea910_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 0, 0, 5;
    %assign/vec4 v0xdea9f0_0, 0;
    %pushi/vec4 5, 0, 3;
    %assign/vec4 v0xdea910_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0xdea910_0;
    %subi 1, 0, 3;
    %assign/vec4 v0xdea910_0, 0;
    %load/vec4 v0xdea840_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.4, 8;
    %pushi/vec4 1, 0, 1;
    %ix/load 5, 0, 0;
    %load/vec4 v0xdea910_0;
    %pad/u 32;
    %subi 1, 0, 32;
    %ix/vec4 4;
    %assign/vec4/off/d v0xdea9f0_0, 4, 5;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0xdeada0_0, 0;
    %jmp T_0.5;
T_0.4 ;
    %pushi/vec4 0, 0, 1;
    %ix/load 5, 0, 0;
    %load/vec4 v0xdea910_0;
    %pad/u 32;
    %subi 1, 0, 32;
    %ix/vec4 4;
    %assign/vec4/off/d v0xdea9f0_0, 4, 5;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0xdeada0_0, 0;
T_0.5 ;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0xdc2310;
T_1 ;
    %wait E_0xdc29d0;
    %load/vec4 v0xdea910_0;
    %dup/vec4;
    %pushi/vec4 5, 0, 3;
    %cmp/u;
    %jmp/1 T_1.0, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 3;
    %cmp/u;
    %jmp/1 T_1.1, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 3;
    %cmp/u;
    %jmp/1 T_1.2, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 3;
    %cmp/u;
    %jmp/1 T_1.3, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 3;
    %cmp/u;
    %jmp/1 T_1.4, 6;
    %pushi/vec4 0, 0, 5;
    %store/vec4 v0xdeab20_0, 0, 5;
    %jmp T_1.6;
T_1.0 ;
    %pushi/vec4 16, 0, 5;
    %store/vec4 v0xdeab20_0, 0, 5;
    %jmp T_1.6;
T_1.1 ;
    %pushi/vec4 8, 0, 5;
    %store/vec4 v0xdeab20_0, 0, 5;
    %jmp T_1.6;
T_1.2 ;
    %pushi/vec4 4, 0, 5;
    %store/vec4 v0xdeab20_0, 0, 5;
    %jmp T_1.6;
T_1.3 ;
    %pushi/vec4 2, 0, 5;
    %store/vec4 v0xdeab20_0, 0, 5;
    %jmp T_1.6;
T_1.4 ;
    %pushi/vec4 1, 0, 5;
    %store/vec4 v0xdeab20_0, 0, 5;
    %jmp T_1.6;
T_1.6 ;
    %pop/vec4 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0xdc6f20;
T_2 ;
    %vpi_call 2 22 "$dumpfile", "sar5.vcd" {0 0 0};
    %vpi_call 2 23 "$dumpvars", 32'sb00000000000000000000000000000000, S_0xdc2310 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xdeb0b0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xdeb340_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xdeb170_0, 0, 1;
    %delay 2, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0xdeb340_0, 0, 1;
    %delay 30, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0xdeb170_0, 0, 1;
    %delay 80, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xdeb170_0, 0, 1;
    %delay 120, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0xdeb170_0, 0, 1;
    %delay 400, 0;
    %vpi_call 2 32 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0xdc6f20;
T_3 ;
    %delay 10, 0;
    %load/vec4 v0xdeb0b0_0;
    %inv;
    %store/vec4 v0xdeb0b0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "sar5_tb.v";
    "sar5.v";
